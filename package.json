{
  "name": "graphql-requester",
  "version": "1.0.0",
  "description": "Axios-based library used to request data from grapql (now with middlewares!)",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "tslint": "tslint --project tsconfig.json --config tslint.json 'src/**/*.ts?(x)'",
    "jest": "jest",
    "test": "npm run tslint && npm run jest",
    "prettier": "prettier --write src/**/*.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s3rious/graphql-requester.git"
  },
  "keywords": [
    "graphql",
    "graphql-client",
    "request",
    "xhr",
    "fetch",
    "axios",
    "middleware"
  ],
  "author": "s3rious a.k.a. Leonid Semenov <canismajor@kaschenka.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s3rious/graphql-requester/issues"
  },
  "homepage": "https://github.com/s3rious/graphql-requester#readme",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/prettier": "^1.16.4",
    "jest": "^24.8.0",
    "jest-mock-axios": "^3.0.0",
    "prettier": "^1.18.2",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.22.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}
